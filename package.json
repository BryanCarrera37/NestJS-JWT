{
  "name": "@bryancm/nestjs-jwt",
  "version": "1.1.0",
  "description": "This is a library made for your projects so that you can have a several functions that you can use to implement the Bearer Authentication.",
  "main": "index.js",
  "scripts": {
    "tsc:init": "tsc --init",
    "copy:files": "cp ./LICENSE ./dist/ && cp ./README.md ./dist/ && cp ./package.json ./dist/",
    "build": "rm -rf dist && tsc && npm run copy:files",
    "publish": "cd dist/ && yarn publish --access public"
  },
  "keywords": [
    "nest",
    "nestjs",
    "package",
    "development",
    "jwt",
    "json",
    "token",
    "bearer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/BryanCarrera37/NestJS-JWT"
  },
  "bugs": {
    "url": "https://github.com/BryanCarrera37/NestJS-JWT/issues"
  },
  "author": {
    "name": "Bryan Carrera",
    "email": "bc00932@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^10.3.8",
    "@nestjs/core": "^10.3.8",
    "@nestjs/jwt": "^10.2.0",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "typescript": "^5.4.5"
  }
}
